.evaluate-title{
  display: flex;
  font-size: 33rpx;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx;
  background: #fff;
  border-bottom: 1px solid #f0f0f0;
}
.evaluate-title text:last-child{
  font-size: 31rpx;
  color: #676767;
}
.star {
  margin-left: 10rpx;
  width: 50rpx;
  height: 50rpx;
}
.select{
  background: #fff;
  padding: 20rpx;
  font-size: 33rpx;
  display: flex;
  justify-content: space-between;
}
.evaluate-content{
  font-size: 31rpx;
  margin: 20rpx 0;
  padding: 20rpx;
  background: #fff;
}
.evaluate-content textarea{
  width: 100%;
}

.evaluate-submit button{
  font-size: 32rpx;
  background: #fff;
  color: #EE6A50;
}