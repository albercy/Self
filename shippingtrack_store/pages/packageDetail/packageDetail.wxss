page {
  height: 100%;
  color: #676767;
  font-size: 33rpx;
}

.logistics {
  height: 100%;
}

.logistics-title {
  line-height: 50rpx;
  padding: 15rpx;
  display: flex;
  flex-direction: column;
  background: #fff;
}

.fromTo {
  margin-top: 10rpx;
  padding-top: 10rpx;
  border-top: 1px solid #676767;
  display: flex;
  flex-direction: column;
}

.logistics-content {
  margin-top: 3%;
  background: #fff;
}

.logistics-list {
  line-height: 60rpx;
  display: flex;
  flex-direction: column;
  padding: 10rpx;
  border-bottom: 1px solid #f0f0f0;
}

.logistics-list text:last-child {
  font-size: 31rpx;
  text-indent: 15rpx;
}

.products-content{
  margin-top: 3%;
  background: #fff;
}

.products-list{
  padding: 10rpx 30rpx;
}

.products-list view{
  display: flex;
  justify-content: space-between
}